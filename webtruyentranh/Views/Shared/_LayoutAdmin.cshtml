                        </li>
                        <li class="sidebar-item">
                            <div class="sidebar-link waves-effect waves-dark sidebar-link">

                                @Html.ActionLink("Quản Lý Thông Tin Liên Hệ", "Index", "LienHe")


                            </div>
                        </li>
                        <li class="sidebar-item">
                            <div class="sidebar-link waves-effect waves-dark sidebar-link">

                                @Html.ActionLink("Quản Lý Tin Tức", "Index", "Tintuc")


                            </div>
                        </li>
                        <li class="sidebar-item">
                            <div class="sidebar-link waves-effect waves-dark sidebar-link">

                                @Html.ActionLink("Quản Lý Đơn Mua Truyện", "Index", "DonHang")


                            </div>
                        </li>
                        <li class="sidebar-item">
                            <div class="sidebar-link waves-effect waves-dark sidebar-link">

                                @Html.ActionLink("Trang Truyện", "Index", "WebTruyen")


                            </div>
                        </li>
                    </ul>
                </nav>
                <!-- End Sidebar navigation -->
            </div>
            <!-- End Sidebar scroll-->
        </aside>
