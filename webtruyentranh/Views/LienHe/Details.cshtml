@model webtruyentranh.Models.LienHe

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}



<div>

    <hr />
    <dl class="dl-horizontal">


        <dd>
            Họ Tên:
            @Html.DisplayFor(model => model.HoTen)
        </dd>



        <dd>
            Địa Chỉ:
            @Html.DisplayFor(model => model.DiaChi)
        </dd>



        <dd>
            Email:
            @Html.DisplayFor(model => model.Email)
        </dd>



        <dd>
            Số Điện Thoại:
            @Html.DisplayFor(model => model.DienThoai)
        </dd>



        <dd>
            Góp Ý:
            @Html.DisplayFor(model => model.GopY)
        </dd>

    </dl>
</div>

<p>

    <a href="@Url.Action("Index","LienHe")" style="background-color: #dc3545; border-radius: 15px" class="btn btn-default">Quay trở lại</a>

</p>
<style>
    dt {
        margin: 0 50px;
    }

    dd {
        margin: 0 50px;
    }

    p {
        margin: 0 50px;
    }
</style>